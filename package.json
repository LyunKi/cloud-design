{
  "private": true,
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "scripts": {
    "test": "jest",
    "test:updateSnap": "pnpm -r --no-bail run test:updateSnap",
    "docs:dev": "dumi dev",
    "docs:build": "pnpm build:components && dumi build",
    "build:components": "pnpm --filter @cloud-design/* --no-bail build",
    "build:ui-creator": "pnpm --filter \"./ui-creator/**\" build",
    "prepare:husky": "husky install"
  },
  "devDependencies": {
    "@changesets/cli": "^2.19.0",
    "@cloud-dragon/eslint-config": "^0.1.0",
    "@cloud-dragon/prettier-config": "^0.0.3",
    "@cloud-dragon/stylelint-config-standard": "^0.0.1",
    "@cloud-dragon/tsconfig-recommended": "^0.0.1",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@testing-library/jest-native": "^4.0.5",
    "@testing-library/react-native": "^11.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^28.1.6",
    "babel-jest": "^28.1.3",
    "dumi": "^1.1.46",
    "eslint": "^8.2.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^10.1.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^10.2.11",
    "metro-react-native-babel-preset": "^0.72.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.2.0",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.0",
    "umi-plugin-react-native": "^0.5.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "pnpm": {
    "overrides": {
      "react": "18.0.0",
      "react-dom": "18.0.0",
      "react-native-web": "0.18.8"
    }
  }
}
